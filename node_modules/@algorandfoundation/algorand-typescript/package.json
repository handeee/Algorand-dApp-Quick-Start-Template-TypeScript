{
  "scripts": {},
  "files": [
    "**"
  ],
  "name": "@algorandfoundation/algorand-typescript",
  "version": "1.0.0-beta.72",
  "description": "This package contains definitions for the types which comprise Algorand TypeScript which can be compiled to run on the Algorand Virtual Machine using the Puya compiler.",
  "private": false,
  "overrides": {
    "cross-spawn": "7.0.5",
    "npm": "10.9.1",
    "esbuild": "0.25.0"
  },
  "peerDependencies": {
    "tslib": "^2.6.2"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./arc4": {
      "types": "./arc4/index.d.ts",
      "import": "./arc4/index.mjs",
      "require": "./arc4/index.js"
    },
    "./op": {
      "types": "./op.d.ts",
      "import": "./op.mjs",
      "require": "./op.js"
    },
    "./itxn": {
      "types": "./itxn.d.ts",
      "import": "./itxn.mjs",
      "require": "./itxn.js"
    },
    "./gtnx": {
      "types": "./gtxn.d.ts",
      "import": "./gtxn.mjs",
      "require": "./gtxn.js"
    }
  }
}
